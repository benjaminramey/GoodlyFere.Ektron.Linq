      <type type="ICommunityGroupManager" mapTo="Cache.ICommunityGroupManager">
        <constructor/>
      </type>

    </container>
    <container name="Default">
      <!-- 
        The default container has search caching turned on and nothing else.
        To enable more caching, copy cache mappings from the cache container into this container for the Managers you wish to cache.
        To cache ALL managers, switch your web.config to use Cache as the default container.
      -->
      <type type="ISearchSettings" mapTo="Cache.ISearchSettings">
        <constructor/>
      </type>
      <type type="ISuggestedResults" mapTo="Cache.ISuggestedResults">
        <constructor/>
      </type>
      <type type="IIntegratedSearchMapping" mapTo="Cache.IIntegratedSearchMapping">
        <constructor/>
      </type>

    </container>
  </containers>
</unity>
